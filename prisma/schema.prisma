datasource db {
    provider = "postgresql"
    url      = "postgresql://albertsmit@localhost:5432/albertsmit"
}

generator db {
    provider = "go run github.com/prisma/prisma-client-go"
}

model Post {
    id        String   @default(cuid()) @id
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    title     String
    published Boolean
    desc      String?
}